{
  "apps": [
    {
      "name": "vicflix-production",
      "script": "ProdServer.js",
      "watch": false,
      "out_file": "/dev/null",
      "error_file": "/dev/null",
      "exec_mode": "cluster",
      "instances": "2",
      "ignore_watch": [".git", "envs", "scripts", ".env"],
      "env": {
        "PORT": 5060,
        "NODE_ENV": "production"
      }
    }
  ]
}
